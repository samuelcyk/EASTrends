tempenv <- inner_join(temp.baamax, temp.dhwmax) %>% 
  inner_join(., temp.sstmax) %>%
  inner_join(., temp.sstmean) %>%
  inner_join(., temp.sstmin) %>%
  inner_join(., temp.sstamax) %>%
  inner_join(., temp.sstamean) %>%
  inner_join(., temp.sstamin)

